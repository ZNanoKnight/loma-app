# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API Configuration
API_TIMEOUT=30000

# OpenAI Configuration (for Phase 3 - AI Recipe Generation)
OPENAI_API_KEY=your_openai_api_key_here

# Stripe Configuration (for Phase 2 - Payments)
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
# Note: STRIPE_SECRET_KEY and STRIPE_WEBHOOK_SECRET should be stored in Supabase secrets, NOT in .env
# Stripe Price IDs (create these in your Stripe Dashboard)
STRIPE_PRICE_ID_WEEKLY=price_xxx_weekly
STRIPE_PRICE_ID_MONTHLY=price_xxx_monthly
STRIPE_PRICE_ID_YEARLY=price_xxx_yearly

# Sentry Configuration (Error Tracking)
SENTRY_DSN=your_sentry_dsn_here

# Environment
NODE_ENV=development
